	public void openEditorForResult() {
		MassifHeapTreeNode element = null;
		
		if (allocs.size() > 1) {
			element = (MassifHeapTreeNode) getResult()[0];
		}
		else if (allocs.size() == 1) {
			element = allocs.get(0);
		}

		if (element != null) {
			MassifPlugin.getDefault().openEditorForNode(element);
		}
	}

