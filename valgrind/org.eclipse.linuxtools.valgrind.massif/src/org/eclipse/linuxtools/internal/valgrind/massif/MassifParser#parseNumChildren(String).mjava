	/*
	 * format is "n[0-9]+:"
	 */
	private Integer parseNumChildren(String string) {
		Integer result = null;
		if (string.length() >= 3) {
			String number = string.substring(1, string.length() - 1);
			if (ValgrindParserUtils.isNumber(number)) {
				result = Integer.parseInt(number);
			}
		}
		return result;
	}

