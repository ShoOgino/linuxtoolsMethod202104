	private String getUnitString(MassifSnapshot[] snapshots2) {
		String result;
		MassifSnapshot snapshot = snapshots[0];
		switch (snapshot.getUnit()) {
		case BYTES:
			result = "B"; //$NON-NLS-1$
			break;
		case INSTRUCTIONS:
			result = "i"; //$NON-NLS-1$
			break;
		default:
			result = "ms"; //$NON-NLS-1$
			break;
		}
		return result;
	}

