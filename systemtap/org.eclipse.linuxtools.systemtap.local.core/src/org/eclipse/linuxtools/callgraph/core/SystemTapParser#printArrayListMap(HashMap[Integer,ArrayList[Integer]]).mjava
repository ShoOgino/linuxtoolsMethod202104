	public void printArrayListMap(HashMap<Integer, ArrayList<Integer>> blah) {
		int amt = 0;
		for (int a : blah.keySet()) {
			amt++;
			MP.print(a + " ::> "); //$NON-NLS-1$
			for (int c : blah.get(a)) {
					System.out.print(c + " ");					 //$NON-NLS-1$
			}
			MP.println(""); //$NON-NLS-1$
		}
	}

