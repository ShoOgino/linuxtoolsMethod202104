	@SuppressWarnings("unchecked")
	private void setFilteredData(ArrayList[] data) {
		this.data = new ArrayList<IDataEntry>();
		
		RowEntry entry;
		Object[] row;
		for(int j,i=0; i<data[0].size(); i++) {
			row = new Object[data.length];
			entry = new RowEntry();
			for(j=0; j<data.length; j++)
				row[j] = data[j].get(i);
			entry.putRow(0, row);
			this.data.add(entry);
		}
	}

